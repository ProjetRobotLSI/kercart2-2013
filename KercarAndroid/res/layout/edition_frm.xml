<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >
    
	<TextView
    android:id="@+id/lblNomE"
    android:layout_width="360dp"
    android:layout_height="90dp"
    android:layout_marginTop="250dp"
	android:layout_marginLeft="70dp"
	android:textIsSelectable="true"/>

   <EditText
   android:id="@+id/txtNomE"
   android:layout_width="wrap_content"
   android:layout_height="wrap_content"
   android:layout_marginTop="180dp"
   android:layout_marginLeft="470dp"
   android:hint="@string/editNomE"
   android:inputType="textMultiLine"
   android:lines="5" />
   
    
    <TextView
    android:id="@+id/lblEmailE"
    android:layout_width="360dp"
    android:layout_height="90dp"
    android:layout_marginTop="450dp"
	android:layout_marginLeft="70dp"/>
    
    <EditText
    android:id="@+id/txtEmailE"
    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:layout_marginTop="380dp"
    android:layout_marginLeft="470dp"
    android:hint="@string/editEmailE"
    android:inputType="textMultiLine"
    android:lines="5" />
    
    
    <TextView
    android:id="@+id/lblRetourDepartE"
    android:layout_width="360dp"
    android:layout_height="90dp"
    android:layout_marginTop="650dp"
	android:layout_marginLeft="70dp"
	android:textSize="30dp"
    android:text="Retour du robot :" />
    
	<CheckBox
	android:id="@+id/cbxRetourDepartE"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_marginTop="650dp"
	android:layout_marginLeft="500dp"
	android:text="@string/cbxRetourE"
	android:checked="true" />
    
    
    
    <TextView
    android:id="@+id/lblPhotoArriveeE"
    android:layout_width="360dp"
    android:layout_height="90dp"
    android:layout_marginTop="850dp"
	android:layout_marginLeft="70dp"
	android:textSize="30dp"
    android:text="Photo Arrivee :" />
    
    <CheckBox
	android:id="@+id/cbxPhotoArriveeE"
	android:layout_width="fill_parent"
	android:layout_height="wrap_content"
	android:layout_marginTop="850dp"
	android:layout_marginLeft="500dp"
	android:text="@string/cbxRetourE"
	android:checked="true" />
    
    <Button
    android:id="@+id/btnSuivantE"
    android:layout_width="150dp"
    android:layout_height="70dp"
    android:layout_marginTop="950dp"
	android:layout_marginLeft="200dp"/>
    
    <Button
    android:id="@+id/btnAnnulerE"
    android:layout_width="150dp"
    android:layout_height="70dp"
    android:layout_marginTop="950dp"
	android:layout_marginLeft="500dp"/>
    
</RelativeLayout>